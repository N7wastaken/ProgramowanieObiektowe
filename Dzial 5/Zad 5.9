using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj wartość n: ");
        if (int.TryParse(Console.ReadLine(), out int n) && n >= 0)
        {
            Console.WriteLine($"Rekurencyjnie: {FibonacciRekurencyjnie(n)}");

            Console.WriteLine($"Iteracyjnie: {FibonacciIteracyjnie(n)}");
        }
        else
        {
            Console.WriteLine("Błąd: Podano nieprawidłową liczbę.");
        }
    }

    static int FibonacciRekurencyjnie(int n)
    {
        if (n <= 1)
        {
            return n;
        }
        else
        {
            return FibonacciRekurencyjnie(n - 1) + FibonacciRekurencyjnie(n - 2);
        }
    }

    static int FibonacciIteracyjnie(int n)
    {
        if (n <= 1)
        {
            return n;
        }

        int a = 0, b = 1, temp = 0;

        for (int i = 2; i <= n; i++)
        {
            temp = a + b;
            a = b;
            b = temp;
        }

        return b;
    }
}
