using System;

class Program
{
    static void Main()
    {
        Console.Write("Podaj liczbę rzeczywistą a: ");
        double a = Convert.ToDouble(Console.ReadLine());

        Console.Write("Podaj liczbę rzeczywistą b: ");
        double b = Convert.ToDouble(Console.ReadLine());

        Console.Write("Podaj liczbę rzeczywistą x: ");
        double x = Convert.ToDouble(Console.ReadLine());

        bool czyNalezy = SprawdzPrzedzial(a, b, x);

        if (czyNalezy)
        {
            Console.WriteLine($"Liczba {x} należy do przedziału ({a}, {b}).");
        }
        else
        {
            Console.WriteLine($"Liczba {x} nie należy do przedziału ({a}, {b}).");
        }
    }

    static bool SprawdzPrzedzial(double a, double b, double x)
    {
        return x > a && x < b;
    }
}
